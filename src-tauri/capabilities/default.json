{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-message",
    "dialog:default",
    "shell:allow-open",
    "shell:allow-execute",
    "opener:default",
    "opener:allow-reveal-item-in-dir"
  ]
}
